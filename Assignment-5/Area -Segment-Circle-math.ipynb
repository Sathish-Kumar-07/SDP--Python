    "from math import sin,pi\n",
    "r=float(input(\"Enter the Radius Of the Circle=\"))\n",
    "a=float(input(\"Enter the angle of the Circle(to 180)\"))\n",
    "area=((r*r)/2)*((pi/180)*a-sin(a))\n",
    "print(\"Area of Segment Of the Circle=\",area)"
