    "def Sum_Square(Num):\n",
    "    Sum=0\n",
    "    for i in range(1,Num+1):\n",
    "        Sum+=(i*i)\n",
    "    return Sum\n",
    "Num=int(input(\"Enter Nth Number=\"))\n",
    "print(\"Sum Of\",Num,\"Natural Numbers Is\",Sum_Square(Num));"
   ]
